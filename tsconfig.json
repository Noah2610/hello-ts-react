{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "baseUrl": "./",
        "paths": {
            "prelude": ["src/prelude"],
            "prelude/*": ["src/prelude/*"]
        }
    },
    "include": [
        "./src/**/*"
    ]
}
